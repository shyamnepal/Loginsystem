@model List<UserRolesModel>
@{
    Layout = "~/views/shared/_Layout.cshtml";
}

</table>

<table class="table">
    <thead>
        <tr>
           
            <th scope="col">UserName</th>
            <th scope="col">Email</th>
            <th scope="col">Role</th>
            <th scope="col">Action</th>
        </tr>
    </thead>
    <tbody>
        @foreach(var item in Model)
        {
            <tr>
               
                <td>@item.Username</td>
                <td>@item.Email</td>
                <td>@item.Role</td>
                <td>

                    @Html.ActionLink("Edit", "GetEditData", new {Userid=item.Id}, new {@class="btn btn-sm btn-success"})
                    @Html.ActionLink("Delete", "DeleteUserRoles", new {Userid=item.Id, roleName=item.Role }, new {@class="btn btn-sm btn-danger"})
                </td>
          
                    
            </tr>
        }
       
       
    </tbody>
</table>